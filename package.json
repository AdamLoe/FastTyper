{
  "name": "fasttyper",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server/server 0.0.0.0",
    "prodStart": "NODE_ENV=production nodemon server/server 0.0.0.0 ",
    "forever": "forever start server/server.js",
    "prodWindows": "set NODE_ENV=production&& nodemon server/server.js",
    "webProd": "webpack --config client/webpack.config.js --mode production",
    "webpack": "webpack-dev-server --port 8080 --hot --config client/webpack.config.js --mode development --host 0.0.0.0",
    "test": "jest --watchAll",
    "prodInstall": "npm install --only=prod",
    "sass": "node-sass -w client/src/scss/index.scss client/src/scss/output.scss"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "^.+\\.svg$": "jest-svg-transformer"
    }
  },
  "nodemonConfig": {
    "ignore": [
      "package*",
      "client/*",
      "node_modules/*"
    ]
  },
